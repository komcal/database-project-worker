{
  "extends": "airbnb-base",
   "rules": {
    "no-console": 0,
    "import/prefer-default-export": 0,
    "no-use-before-define": "off",
    "no-param-reassign": 0,
    "no-shadow": "off",
    "func-name": "off",
    "global-require": "off",
    "max-len": "off",
    "new-cap": ["error", { "capIsNewExceptionPattern": "^Router" }],
    "comma-dangle": ["error", "never"],
    "no-underscore-dangle": ["error", { "allow": ["_id"] } ]
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "plugin": [
    "import"
  ],
  "globals": {
    "test": true,
    "expect": true
  }
}
